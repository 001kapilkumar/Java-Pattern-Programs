public class Pattern_10 {
    public static void main(String [] args) {
        int row = 10 ;
        for(int i = 1 ; i<= row ; i++){
            for(int j = 0 ; j<row ; j++){
                if(i==1 || j == 0 || j == (row-1) || i==row){
                    System.out.print("1" ) ;
                }
                else{
                    System.out.print(" ") ;
                }
            }
            System.out.println() ;
        }
    }
}

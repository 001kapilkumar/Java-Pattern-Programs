public class Pattern_12 {
    public static void main(String [] args){
        int row = 5 ;
        for(int i = row; i>0 ; i--){
            for(int j = row; j>i ; j--){
                System.out.print(" ") ;
            }
            for(int j = 0 ; j<(2*i-1) ; j++){
                System.out.print("*") ;
            }
            System.out.println();
        }
    }
}

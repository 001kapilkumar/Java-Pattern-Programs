public class Pattern_3 {
    public static void main(String [] args){
        for(int i = 1; i<11 ; i++){
            int n = 1 ;
            for(int j = 0 ; j<i ; j++){
                System.out.print(n*i + " ") ;
                n++ ;
            }
            System.out.println();
        }
    }
}

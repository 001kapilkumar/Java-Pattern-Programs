public class Pattern_7 {
    public static void main(String [] args){
        int row = 9 ;
        System.out.println(0);
        for(int i = row; i>0 ; i--){
            for(int j = i ;j<=row ; j++ ){
                System.out.print(j) ;
            }
          System.out.print(0) ;
            for(int j = row ; j>=i ; j--){
                System.out.print(j) ;
            }
            System.out.println() ;
        }
    }
}
